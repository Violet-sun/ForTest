          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="script">import java.io.File;
import org.apache.jmeter.threads.JMeterContextService; 
String line = vars.get(&quot;line&quot;);
int po = Integer.parseInt(line);

File testFile = new File(&quot;D:\\session.txt&quot;);
RandomAccessFile demo = new RandomAccessFile(testFile,&quot;rw&quot;);
int tmp = testFile.length();
String t = tmp.toString();
int len = testFile.length() -33;
vars.put(&quot;len&quot;,t);

if(po &gt; len){
  JMeterContextService.getContext().getThread().stop();
}else{
	demo.seek(po);
	byte[] n = new byte[32];
	demo.read(n);
	String session = new String(n,0,32);
	vars.put(&quot;session&quot;,session);
demo.close(); 
	}



</stringProp>
